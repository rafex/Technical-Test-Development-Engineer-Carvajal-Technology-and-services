{
  "name": "examen",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel serve watch:ts watch:scss",
    "serve": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "lint": "npm-run-all lint:ts lint:scss lint:typecheck",
    "lint:typecheck": "ng lint --type-check",
    "lint:scss": "stylelint 'src/styles/**/*.scss' --syntax scss || true",
    "lint:ts": "eslint src/app/. --ext .ts",
    "watch": "ng build --watch --configuration development",
    "watch:scss": "onchange 'src/styles/**/*.scss' -- npm-run-all lint:scss",
    "watch:ts": "onchange 'src/app/**/*.ts' -- npm-run-all lint:ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "bootstrap": "^5.0.2",
    "jquery": "^3.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "stylelint": "^13.13.1",
    "stylelint-order": "^4.1.0",
    "tslint": "^6.1.3",
    "typescript": "~4.3.2"
  }
}
